    <script>

        function Checksize(filesize) {
            if (filesize > 209715200) // 200 MB for bytes.
            {
                alert("File size must under 200MB!");
                return;
            }
            else {
                //    alert("File size less than 200MB!");
            }
        }
        function CheckFile(input) {
            if (input.files[0] != null) {
                var validExtensions = ['pdf', 'jpg', 'png', 'jpeg'];
                var fileName = input.files[0].name;
                var fileNameExt = fileName.substr(fileName.lastIndexOf('.') + 1).toLowerCase();
                if ($.inArray(fileNameExt, validExtensions) == -1) {
                    input.type = ''
                    input.type = 'file'
                    //$('#user_img').attr('src', "");
                    alert("Only these file types are accepted : " + validExtensions.join(', '));
                }
                else {
                    Checksize(input.files[0].size);
                    if (input.files && input.files[0]) {
                        var filerdr = new FileReader();
                        filerdr.onload = function (e) {
                            //   $('#user_img').attr('src', e.target.result);
                        }
                        filerdr.readAsDataURL(input.files[0]);
                    }
                }
            }
        }
        function show(input) {
            if (input.files[0] != null) {
                var validExtensions = ['jpg', 'png', 'jpeg', 'zip'];
                var fileName = input.files[0].name;
                var fileNameExt = fileName.substr(fileName.lastIndexOf('.') + 1).toLowerCase();
                if ($.inArray(fileNameExt, validExtensions) == -1) {
                    input.type = ''
                    input.type = 'file'
                    $('#user_img').attr('src', "");
                    alert("Only these file types are accepted : " + validExtensions.join(', '));
                }
                else {
                    Checksize(input.files[0].size);
                    if (input.files && input.files[0]) {
                        var filerdr = new FileReader();
                        filerdr.onload = function (e) {
                            $('#user_img').attr('src', e.target.result);
                        }
                        filerdr.readAsDataURL(input.files[0]);
                    }
                }
            }
        }

        function showDocument(input) {
            if (input.files[0] != null) {
                var validExtensions = ['jpg', 'png', 'jpeg', 'zip', 'pdf'];
                var fileName = input.files[0].name;
                var fileNameExt = fileName.substr(fileName.lastIndexOf('.') + 1).toLowerCase();
                if ($.inArray(fileNameExt, validExtensions) == -1) {
                    input.type = ''
                    input.type = 'file'
                    $('#user_img').attr('src', "");
                    alert("Only these file types are accepted : " + validExtensions.join(', '));
                }
                else {
                    Checksize(input.files[0].size);
                    if (input.files && input.files[0]) {
                        var filerdr = new FileReader();
                        filerdr.onload = function (e) {
                            $('#user_img').attr('src', e.target.result);
                        }
                        filerdr.readAsDataURL(input.files[0]);
                    }
                }
            }
        }
        function CheckFileExcel(input) {
            if (input.files[0] != null) {
                var validExtensions = ['xls', 'xlsx'];
                var fileName = input.files[0].name;
                var fileNameExt = fileName.substr(fileName.lastIndexOf('.') + 1).toLowerCase();
                if ($.inArray(fileNameExt, validExtensions) == -1) {
                    input.type = ''
                    input.type = 'file'
                    alert("Only these file types are accepted : " + validExtensions.join(', '));
                }
                else {
                    // Checksize(input.files[0].size);
                    if (input.files && input.files[0]) {
                        var filerdr = new FileReader();
                        filerdr.onload = function (e) {
                            $('#user_img').attr('src', e.target.result);
                        }
                        filerdr.readAsDataURL(input.files[0]);
                    }
                }
            }
        }

    </script>

    <script type="text/javascript">
        $(function () {
            if ($('#CertificateStatus').val() != 'Yes') {
                $('#CertificationImageStatus').hide();
            }
            $('#CertificateStatus').change(function () {
                if ($('#CertificateStatus').val() != 'Yes') {
                    $('#CertificationImageStatus').hide();
                } else {
                    $('#CertificationImageStatus').show();
                }
            });

        });

        $(function () {
            var message = $('#IsPasswordResetWithPan').val();
            //alert(message);
            if ($('#IsPasswordResetWithPan').val() == 'No') {
                $('#IsPasswordResetWithPanStatus1').show();
                $('#IsPasswordResetWithPanStatus2').show();
            } else {
                $('#IsPasswordResetWithPanStatus1').hide();
                $('#IsPasswordResetWithPanStatus2').hide();
            }

        });

    </script>
