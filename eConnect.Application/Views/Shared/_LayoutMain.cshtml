<body>
    <div id="app">
        <div class="main-wrapper">
            <nav class="navbar navbar-expand-lg main-navbar fixed-top">
                <ul class="navbar-nav mr-3">
                    <li>
                        <a href="#" data-toggle="sidebar" class="nav-link nav-link-lg menu-icon">
                            <i class="mdi mdi-menu text-dark"></i>
                        </a>
                    </li>
                </ul>
                <div class="d-flex mr-3">
                    <h6 class="mr-3">
                        @ViewBag.Title
                    </h6>
                    <div class="notification">
                        <marquee behavior="scroll" scrollamount="5" direction="left" onmouseover="this.stop();" onmouseout="this.start();">
                            <span>
                                <a href="#" data-toggle="modal" data-target="#newsdetail">
                                    @{ Html.RenderAction("_GetAnnouncementMessage", "Partial"); }
                                </a>
                                @*<span>   @{ Html.RenderAction("_GetAnnouncementMessage", "Partial"); }</span>*@
                            </span>
                        </marquee>
                    </div>
                </div>

                <ul class="navbar-nav navbar-right ml-auto">


                    <li class="dropdown">
                        <a href="#" data-toggle="dropdown" class="nav-link dropdown-toggle nav-link-lg nav-link-user ml-3">
                            @{

                                if (Session["ProfileImage"].ToString() != "NoImage")
                                {
                                    string picpath = "/UploadedFiles" + "/" + Session["ProfileImage"].ToString();
                                    <img src="@picpath" alt="User Image" class="rounded-circle mr-1" />

                                }
                                else
                                {
                                    <img src="~/Content/Image/ProfileImage.jfif" alt="User Image" class="rounded-circle mr-1" />
                                }
                            }
                            <div class="d-sm-none d-lg-inline-block">@Session["UserName"] (@Session["UserRoleName"])</div>
                        </a>
                        <div class="dropdown-menu dropdown-menu-right">
                            <div class="dropdown-title">
                                Last Login at :
                                @Session["LastLoginDetails"]
                            </div>
                            <a href="@Url.Action("ResetPassword", "Home")" class="dropdown-item has-icon">
                                <i class="mdi mdi-account-outline"></i> Change Password
                            </a>
                            <div class="dropdown-divider"></div>
                            <a href="@Url.Action("Logout", "Home")" class="dropdown-item has-icon text-danger">
                                <i class="mdi mdi-logout"></i> Logout
                            </a>
                        </div>
                    </li>
                </ul>
            </nav>
            <div class="main-sidebar">
                <aside id="sidebar-wrapper">
                    <div class="sidebar-brand">
                        <a href=""><img src="~/Content/HTML/img/e-gramin-logo.png" alt=""></a>
                    </div>
                    <div class="sidebar-brand sidebar-brand-sm">
                        <a href="Dashboard.html"><img src="~/Content/HTML/img/favicon.png" alt=""></a>
                    </div>

                    @{ Html.RenderAction("_GetAppMenu", "Partial"); }

                </aside>
            </div>

            <!-- Main Content -->
            <div class="main-content">
                @RenderBody()
            </div>
            <footer class="main-footer bg-light">
                <div class="footer-left">
                    &copy; 2021-2022 .All RIght Reserved. <a href="#">eGramin</a>
                </div>
                <div class="footer-right">
                    <a href=""><i class="mdi mdi-facebook-box facebook"></i></a>
                    <a href=""><i class="mdi mdi-twitter-box twitter"></i></a>
                    <a href=""><i class="mdi mdi-rss-box rss"></i></a>
                </div>
            </footer>
        </div>
    </div>
    <div class="modal fade" id="newsdetail" tabindex="-1" aria-labelledby="new-detailLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">

                <div class="modal-body p-4">
                    @{ Html.RenderAction("_GetAnnouncementDetail", "Partial"); }
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-warning px-4 text-white" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>



