<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, shrink-to-fit=no" name="viewport">
    <title>Report</title>

    <!-- General CSS Files -->
    <link rel="stylesheet" href="~/Content/HTML/scss/custom.css">
    <link rel="stylesheet" href="~/Content/HTML/node_modules/mdi/font/css/materialdesignicons.min.css">
    <!-- CSS Libraries -->
    <link rel="stylesheet" href="https://cdn.datatables.net/1.10.25/css/dataTables.bootstrap4.min.css">
    <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/daterangepicker/daterangepicker.css" />
    <link href="~/Content/HTML/css/jquery.growl.css" rel="stylesheet" type="text/css">
    <link href="~/Content/HTML/css/fileup.css" rel="stylesheet" type="text/css">

</head>

<body>
    <div id="app">
        <div class="main-wrapper">
            <nav class="navbar navbar-expand-lg main-navbar fixed-top">
                <ul class="navbar-nav mr-3">
                    <li>
                        <a href="#" data-toggle="sidebar" class="nav-link nav-link-lg menu-icon">
                            <i class="mdi mdi-menu text-dark"></i>
                        </a>
                    </li>
                </ul>
                <div class="d-flex mr-3">
                    <h6 class="mr-3">
                        @ViewBag.Title
                    </h6>
                    <div class="notification">
                        <marquee behavior="scroll" scrollamount="5" direction="left" onmouseover="this.stop();" onmouseout="this.start();">
                            <span>
                                <a href="#" data-toggle="modal" data-target="#newsdetail">
                                    @{ Html.RenderAction("_GetAnnouncementMessage", "Partial"); }
                                </a>
                                <span>   @{ Html.RenderAction("_GetAnnouncementMessage", "Partial"); }</span>
                            </span>
                        </marquee>
                    </div>
                </div>

                <ul class="navbar-nav navbar-right ml-auto">


                    <li class="dropdown">
                        <a href="#" data-toggle="dropdown" class="nav-link dropdown-toggle nav-link-lg nav-link-user ml-3">
                            <img alt="image" src="~/Content/HTML/img/avatar-1.png" class="rounded-circle mr-1">
                            <div class="d-sm-none d-lg-inline-block">@Session["UserName"] (@Session["UserRoleName"])</div>
                        </a>
                        <div class="dropdown-menu dropdown-menu-right">
                            <div class="dropdown-title">
                                Last Login at :
                                @Session["LastLoginDetails"]
                            </div>
                            <a href="@Url.Action("ResetPassword", "Home")" class="dropdown-item has-icon">
                                <i class="mdi mdi-account-outline"></i> Change Password
                            </a>
                            <div class="dropdown-divider"></div>
                            <a href="@Url.Action("Logout", "Home")" class="dropdown-item has-icon text-danger">
                                <i class="mdi mdi-logout"></i> Logout
                            </a>
                        </div>
                    </li>
                </ul>
            </nav>
            <div class="main-sidebar">
                <aside id="sidebar-wrapper">
                    <div class="sidebar-brand">
                        <a href=""><img src="~/Content/HTML/img/e-gramin-logo.png" alt=""></a>
                    </div>
                    <div class="sidebar-brand sidebar-brand-sm">
                        <a href="Dashboard.html"><img src="~/Content/HTML/img/favicon.png" alt=""></a>
                    </div>

                    @{ Html.RenderAction("_GetAppMenu", "Partial"); }

                </aside>
            </div>

            <!-- Main Content -->
            <div class="main-content">
                @RenderBody()
            </div>
            <footer class="main-footer bg-light">
                <div class="footer-left">
                    &copy; 2021-2022 .All RIght Reserved. <a href="#">eGramin</a>
                </div>
                <div class="footer-right">
                    <a href=""><i class="mdi mdi-facebook-box facebook"></i></a>
                    <a href=""><i class="mdi mdi-twitter-box twitter"></i></a>
                    <a href=""><i class="mdi mdi-rss-box rss"></i></a>
                </div>
            </footer>
        </div>
    </div>
    <div class="modal fade" id="newsdetail" tabindex="-1" aria-labelledby="new-detailLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">

                <div class="modal-body p-4">
                    @{ Html.RenderAction("_GetAnnouncementDetail", "Partial"); }
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-warning px-4 text-white" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>



    <!-- General JS Scripts -->
    <script src="~/Content/HTML/js/jquery.min.js"></script>
    <script src="~/Content/HTML/node_modules/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/Content/HTML/js/jquery.nicescroll.min.js"></script>
    <script src="~/Content/HTML/js/moment.min.js"></script>

    <!-- Template JS File -->
    <script src="~/Content/HTML/js/scripts.js"></script>
    <script>
        $(function () {
            $('marquee').mouseover(function () {
                $(this).attr('scrollamount', 0);
            }).mouseout(function () {
                $(this).attr('scrollamount', 5);
            });
        });
    </script>
    <!-- JS Libraies -->
    <script src="https://cdn.datatables.net/1.10.25/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/1.10.25/js/dataTables.bootstrap4.min.js"></script>
    <!-- Page Specific JS File -->
    <script src="~/Content/HTML/js/modules-datatables.js"></script>
    <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/daterangepicker/daterangepicker.min.js"></script>
    <script src="~/Content/HTML/js/jquery.growl.js"></script>
    <script src="~/Content/HTML/js/fileup.js"></script>

    @*<script src="https://code.jquery.com/jquery-3.5.1.js"></script>*@
    <script src="https://cdn.datatables.net/1.10.25/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/1.7.1/js/dataTables.buttons.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/pdfmake.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/vfs_fonts.js"></script>
    <script src="https://cdn.datatables.net/buttons/1.7.1/js/buttons.html5.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/1.7.1/js/buttons.print.min.js"></script>
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.25/css/jquery.dataTables.min.css" />
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/buttons/1.7.1/css/buttons.dataTables.min.css" />

    <script>
        $(function () {
            $('.expiry-date').daterangepicker({
                singleDatePicker: true,
                showDropdowns: true,
                minYear: 1901,
                autoApply: true,
                maxYear: parseInt(moment().format('YYYY'), 10)
            });
        });
        $(function () {
            $('.datetimepicker').datepicker();
        });

        $(document).ready(function () {
            setTimeout(function () {
                $("#SuccessMsg").fadeOut();
            }, 10000);
        });
    </script>
    <script>
        $.fileup({
            url: "https://localhost:44331//Content/Images/CSP",
            inputID: 'upload-1',
            queueID: 'upload-1-queue',
            onSuccess: function (response, file_number, file) {
                $.growl.notice({
                    title: "Upload success!",
                    message: file.name
                });
            },
            onError: function (event, file, file_number) {
                $.growl.error({
                    message: "Upload error!"
                });
            }
        });
