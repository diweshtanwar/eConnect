<section class="section">


    @using (@Html.BeginForm("IndexSearch", "CSP", FormMethod.Get))
    {@Html.AntiForgeryToken()


    <div class="card">
        <div class="card-header">
            <h4><span class="toggle-collapse" data-toggle="collapse" href="#filter" role="button" aria-expanded="false" aria-controls=" filter"></span> Filter</h4>
        </div>
        <div class="card-body collapse" id="filter">
            <div class="row">
                <div class="col-md-3">
                    <div class="form-group">
                        <label>CSP Code</label>
                        <input type="text" id="CSPCode" name="CSPCode" class="form-control" value="@Request["CSPCode"]">
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-group">
                        <label>Name</label>
                        <input type="text" id="CSPName" name="CSPName" class="form-control" value="@Request["CSPName"]">
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-group">
                        <label>Branch Code</label>
                        <input type="text" id="BranchCode" name="BranchCode" class="form-control" value="@Request["BranchCode"]">

                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-group">
                        <label>Status</label>
                        @Html.DropDownList("Status", (IEnumerable<SelectListItem>)ViewBag.Status, " ", new { @class = "form-control" })
                    </div>
                </div>

                <div class="col-md-3">
                    <div class="form-group">
                        <label>Certificate Status</label>
                        @Html.DropDownList("CertificateStatus", (IEnumerable<SelectListItem>)ViewBag.CertificateStatus, " ", new { @class = "form-control" })

                    </div>
                </div>
                <div class="col-md-3 mt-md-4 pt-1">
                    @*<button class="btn btn-dark px-4 mr-2">Cancel</button>*@
                    <button type="submit" class="btn btn-primary px-4">Search</button>
                </div>

            </div>
        </div>
    </div>
}
    <p>
